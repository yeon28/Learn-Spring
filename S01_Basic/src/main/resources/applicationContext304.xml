				<prop key="둘리">서울시 강남구 삼성동</prop>
				<prop key="도우너">서울시 강남구 역삼동</prop>
				<prop key="또치">서울시 송파구 가락동</prop>
			</props>
		</property>
	</bean>


</beans>
